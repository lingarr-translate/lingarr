{
    "semi": false,
    "tabWidth": 4,
    "useTabs": false,
    "printWidth": 100,
    "endOfLine": "auto",
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true,
    "arrowParens": "always",
    "htmlWhitespaceSensitivity": "ignore",
    "bracketSameLine": true,
    "plugins": ["prettier-plugin-tailwindcss"]
}
